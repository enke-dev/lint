{
  "name": "@enke.dev/lint",
  "version": "0.11.17",
  "description": "Meta package to provide linting for web projects",
  "homepage": "https://github.com/enke-dev/lint",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enke-dev/lint.git"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/eslint.config.d.ts",
      "import": "./dist/eslint.config.js",
      "require": "./dist/eslint.config.cjs"
    },
    "./*.js": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    },
    "./eslint": {
      "types": "./dist/eslint.config.d.ts",
      "import": "./dist/eslint.config.js",
      "require": "./dist/eslint.config.cjs"
    },
    "./prettier": {
      "types": "./dist/prettier.config.d.ts",
      "import": "./dist/prettier.config.js",
      "require": "./dist/prettier.config.cjs"
    },
    "./stylelint": {
      "types": "./dist/stylelint.config.d.ts",
      "import": "./dist/stylelint.config.js",
      "require": "./dist/stylelint.config.cjs"
    }
  },
  "scripts": {
    "check": "tsc -p tsconfig.build.json --noEmit",
    "lint": "eslint -c test.eslint.config.ts .",
    "test": "node --experimental-strip-types --test test.run.ts",
    "inspect": "eslint-config-inspector --config eslint.config.ts",
    "dev": "vite build --watch",
    "build": "vite build"
  },
  "author": {
    "name": "David Enke",
    "email": "david@enke.dev"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": "^9.35.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "optionalDependencies": {
    "stylelint": "^16.26.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-order": "^7.0.1"
  },
  "dependencies": {
    "@awmottaz/prettier-plugin-void-html": "2.0.0",
    "@eslint/compat": "2.0.0",
    "@eslint/js": "9.39.2",
    "@eslint/json": "0.14.0",
    "@html-eslint/eslint-plugin": "0.52.1",
    "@html-eslint/parser": "0.52.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-html": "8.1.3",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-import-extensions": "0.1.5",
    "eslint-plugin-lit": "2.1.1",
    "eslint-plugin-lit-a11y": "5.1.1",
    "eslint-plugin-package-json": "0.88.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "eslint-plugin-unused-imports": "4.3.0",
    "eslint-plugin-wc": "3.0.2",
    "prettier-plugin-css-order": "2.2.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "1.4.2",
    "@types/node": "24.10.4",
    "eslint": "9.39.2",
    "prettier": "3.7.4",
    "stylelint": "16.26.1",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-standard-scss": "16.0.0",
    "stylelint-order": "7.0.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "7.3.0",
    "vite-plugin-static-copy": "3.1.4"
  }
}
